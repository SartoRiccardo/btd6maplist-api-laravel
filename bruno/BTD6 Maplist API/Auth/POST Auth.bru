meta {
  name: POST Auth
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/auth
  body: none
  auth: bearer
}

headers {
  Authorization: Bearer {{USER_AUTH_TOKEN}}
}

script:post-response {
  if (res.status === 200) {
    bru.setEnvVar("USER_AUTH_TOKEN", "fake_discord_token");
    console.log("User profile:", JSON.stringify(res.body, null, 2));
  }
}
