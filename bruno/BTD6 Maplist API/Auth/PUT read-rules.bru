    "auth": {
      "type": "bearer",
      "token": "{{USER_AUTH_TOKEN}}"
    },
    "body": {
      "mode": "none"
    },
    "headers": [
      {
        "name": "Accept",
        "value": "application/json",
        "enabled": true
      }
    ]
  }
}
